name: naive_python

docker_env:
  image: python:3.10

#python_env: python_env.yaml

entry_points:
  train:
    parameters:
      train_data: path
      model_output_file: path
    command: "python train.py {train_data} {model_output_file}"
  predict:
    parameters:
      future_data: path
      model: path
      out_file: path
    command: "python predict.py {future_data} {model} {out_file}"
