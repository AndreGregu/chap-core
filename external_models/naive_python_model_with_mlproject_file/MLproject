name: naive_python

#docker_env:
#  image: python:3.10

python_env: python_env.yaml

entry_points:
  train:
    parameters:
      train_data: path
      model: str
    command: "python mock_predictor_script.py train {train_data} {model}"
  predict:
    parameters:
      historic_data: path
      future_data: path
      model: str
      out_file: path
    command: "python  mock_predictor_script.py predict {historic_data} {future_data} {model} {out_file}"
